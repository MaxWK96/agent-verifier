# Moltbook API key — obtained by running: npm run register
MOLTBOOK_API_KEY=

# OpenWeatherMap free-tier key — https://openweathermap.org/api
OPENWEATHER_API_KEY=

# Sepolia wallet private key (WITHOUT 0x prefix, or with — both work)
PRIVATE_KEY=

# Alchemy / Infura Sepolia RPC URL
NEXT_PUBLIC_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_KEY_HERE

# Deployed VerdictRegistry address — filled automatically by npm run deploy:contract
VERDICT_REGISTRY_ADDRESS=

# Optional — CoinGecko Pro key for higher rate limits (free tier works without this)
COINGECKO_API_KEY=

# JSONBin.io — cloud storage so the Vercel deployment shows real verdicts
# 1. Sign up at https://jsonbin.io  2. Create a new bin with initial value []
# 3. Copy the Bin ID from the URL and your Master Key from Account > API Keys
JSONBIN_BIN_ID=
JSONBIN_API_KEY=
